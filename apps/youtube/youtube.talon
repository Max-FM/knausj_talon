title: /youtube.com/
-
# Playback
(play | pause) [video]: key('k')
skip [forward]: key('l')
small skip [forward]: key('right')
skip back: key('j')
small [skip] back: key('left')
speed up: key('>')
slow down: key('<')
# Subtitles and Closed Captions
captions: key('c')
# General
full screen: key('f')
(big | small) mode: key('t')
mini player: key('i')
mute: key('m')
